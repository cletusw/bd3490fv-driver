/dts-v1/;
/plugin/;

/ {
    compatible = "brcm,bcm2835";

    fragment@0 {
        target = <&i2c1>;
        __overlay__ {
            #address-cells = <1>;
            #size-cells = <0>;

            my-amp@76 {
                status = "okay";
                compatible = "ti,tas5756";
                reg = <0x4c>;
            };
        };
    };
};
